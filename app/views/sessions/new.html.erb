<h1>User Login</h1>

<%= form_tag sessions_path do %>
    <div>
      <%= label_tag :email %>
      <%= text_field_tag :email %>
    </div>
    <div>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <div>
      <%= submit_tag("login") %>

    </div>


<% end %>